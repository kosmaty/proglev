<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<BorderPane prefHeight="768" prefWidth="1024" style="-fx-background-color: white;" stylesheets="@proglev.css"
            xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.proglev.gui.ProgLevController">
    <center>
        <VBox>
            <HBox styleClass="proglev-main-menu" BorderPane.alignment="CENTER">
                <Button mnemonicParsing="false" text="%pregnancyList" onAction="#showTable"/>
                <Button mnemonicParsing="false" text="%addPregnancy" onAction="#showAddPregnancy"/>
            </HBox>
            <BorderPane fx:id="mainPanel"/>
        </VBox>
    </center>
</BorderPane>
